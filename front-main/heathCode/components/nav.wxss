/* components/nav.wxss */
.icons{
  display:flex;
  flex-wrap: wrap;
  justify-content: center;
  padding-bottom: 20px;
}
.nav{
  align-items: center;
  text-align: center;
  width: 30%;
}
.nav .imgbg{
  background-color: #C7E4FE;
  width:67px;
  height:67px;
  border-radius: 50%;
  margin: 5px auto;
  display: flex;
  justify-content: center;
  align-items: center;
}
.nav .imgbg image{
  width: 42px;
  height: 42px;
}
.nav text{
  /** 文本1 */
font-size: 14px;
font-weight: 400;
letter-spacing: 0px;
line-height: 19.21px;
text-align: center;
vertical-align: top;
}