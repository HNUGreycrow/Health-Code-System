/* pages/code/code.wxss */


.module{
  border-radius: 5px;
  background: #FFFFFF;
  margin: 24px;
  box-shadow:0px 8px 24px rgba(149,157,165,0.2);
  padding: 12px 27px;
  position: relative;
}
.module-tittle{
  /** 文本1 */
font-size: 16px;
font-weight: 400;
letter-spacing: 0px;
line-height: 32px;
/* margin: 12px 0; */
vertical-align: top;
color: rgba(51, 51, 51, 1);
}
.module-content{
  /** 文本1 */
font-size: 13px;
font-weight: 400;
letter-spacing: 0px;
line-height: 18px;
color: rgba(99, 99, 99, 1);
}

.module .info{
  right: 10px;
  bottom: 10px;
  height: 17px;
  /** 文本1 */
  font-size: 12px;
  font-weight: 400;
  letter-spacing: 0px;
  line-height: 16.46px;
  color: rgba(84, 179, 122, 1);
  text-align: left;
  vertical-align: top;
  position: absolute;
}